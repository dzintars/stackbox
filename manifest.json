{
  "manifest_version": 2,
  "name": "Stack Box",
  "description": "Add links to your StackBox",
  "version": "0.0.1",
  "icons": {
    "128": "dzintars.dev.icon.128.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "dzintars.dev.icon.19.png",
    "default_popup": "popup.html",
    "default_title": "StackBox"
  }
}
